{
  "name": "bundler",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/preset-env": "^7.15.0",
    "@blueprintjs/core": "^3.47.0",
    "@blueprintjs/select": "^3.19.1",
    "@react-leaflet/core": "^2.1.0",
    "copy-to-clipboard": "3.3.3",
    "create-react-class": "^15.7.0",
    "esri-leaflet": "link:./custom_modules/esri-leaflet",
    "esri-leaflet-renderers": "link:./custom_modules/esri-leaflet-renderers",
    "esri-leaflet-vector": "3.1.5",
    "leaflet": "1.9.0",
    "leaflet-draw": "^1.0.4",
    "leaflet-easyprint": "link:./custom_modules/leaflet-easyprint",
    "leaflet-shape-markers": "^1.0.6",
    "leaflet.nontiledlayer": "1.0.9",
    "proj4": "^2.7.5",
    "proj4leaflet": "^1.0.2",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "18.2.0",
    "react-esri-leaflet": "2.0.1",
    "react-leaflet": "4.2.0",
    "react-leaflet-coordinates": "^1.0.1",
    "react-leaflet-sidebarv2": "^0.6.0",
    "react-select": "^5.1.0",
    "react-sizeme": "^3.0.1",
    "react-transition-group": "4.4.2",
    "shadow-cljs": "2.19.9",
    "socks": "2.7.3",
    "vega": "^5.32.0",
    "vega-embed": "^6.21.0",
    "vega-lite": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "babel-loader": "^8.2.3",
    "node-sass": "^7.0.3",
    "node-sass-package-importer": "^5.3.2",
    "parcel": "^2.9.3",
    "sass": "^1.58.3"
  },
  "scripts": {
    "watch": "npx shadow-cljs watch dev",
    "build-js": "babel src/ui --out-dir src/gen --source-maps inline",
    "watch-js": "babel src/ui --out-dir src/gen --source-maps inline --watch",
    "build-worker": "parcel build src/worker/esri-leaflet-worker.js --dist-dir resources/public/js --no-content-hash",
    "build-css": "npx sass --load-path=. --style compressed src/index.scss resources/public/css/style.npm-packaged.css",
    "build-region-report-css": "npx sass --load-path=. --style compressed ../wordpress/plugins/region-report/style.scss ../wordpress/plugins/region-report/style.css",
    "build-blueprint-css": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js -o resources/public/css src/ui/blueprint.scss",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}
